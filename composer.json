{
    "name": "wangzaron/easypay",
    "description": "aggregate payment for alipay ,wechatpay , unionpay and so on",
    "type": "library",
    "require": {
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.23",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/http-foundation": "^2.7|^3.0|^4.0",
        "symfony/cache": "^3.3|^4.0",
        "symfony/psr-http-message-bridge": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zoran Wang",
            "email": "skyandsunning@163.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EasyPay\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php",
            "src/Kernel/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyPay\\Tests\\": "tests/"
        }
